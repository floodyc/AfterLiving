{
  "name": "@legacyvideo/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "pnpm --filter @legacyvideo/db build && pnpm --filter @legacyvideo/shared build && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@legacyvideo/db": "workspace:*",
    "@legacyvideo/shared": "workspace:*",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "ioredis": "5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "tsx": "^4.7.0",
    "vitest": "^1.1.1"
  }
}

